:cmock:
    :mock_prefix: mock_
    :mock_path: @MOCKPATH@
    :when_no_prototypes: :warn
    :when_ptr: :compare_ptr
    :plugins:
        - :callback
        - :expect_any_args
        - :ignore
        - :ignore_arg
    :treat_as:
        uint8:    HEX8
        uint16:   HEX16
        uint32:   UINT32
        int8:     INT8
